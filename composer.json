{
  "name": "xman12/persistent-request",
  "description": "Library for create persistent request based on laravel framework",
  "minimum-stability": "stable",
  "license": "LGPL-3.0-or-later",
  "type": "library",
  "keywords": [
    "laravel",
    "guzzle",
    "persistent-request",
    "request"
  ],
  "authors": [
    {
      "name": "xman12",
      "email": "xman12@mail.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": ">=6.3",
    "laravel/framework": ">=7.0",
    "laravel/serializable-closure": "^1.*"
  },
  "autoload": {
    "psr-4": {
      "PersistentRequest\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PersistentRequest\\Providers\\PersistentRequestServiceProvider"
      ]
    }
  }
}